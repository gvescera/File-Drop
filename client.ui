<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Frame" id="frmMain">
    <property name="height">200</property>
    <property name="padx">0</property>
    <property name="pady">0</property>
    <property name="width">200</property>
    <layout>
      <property name="column">0</property>
      <property name="ipadx">0</property>
      <property name="padx">10</property>
      <property name="pady">10</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
      <columns>
        <column id="1">
          <property name="minsize">0</property>
        </column>
      </columns>
    </layout>
    <child>
      <object class="tk.Label" id="lbTitle">
        <property name="borderwidth">0</property>
        <property name="font">{Arial} 16 {}</property>
        <property name="text" translatable="yes">File Drop</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">2</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Entry" id="entPath">
        <property name="font">{Arial} 12 {}</property>
        <property name="text" translatable="yes">Choose a file to drop...</property>
        <layout>
          <property name="column">0</property>
          <property name="ipadx">85</property>
          <property name="padx">5</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="btnDrop">
        <property name="command">drop</property>
        <property name="font">{Arial} 10 {}</property>
        <property name="text" translatable="yes">Drop</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">2</property>
          <property name="ipadx">25</property>
          <property name="ipady">5</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="btnOpen">
        <property name="command">askfilepath</property>
        <property name="text" translatable="yes">&gt;</property>
        <layout>
          <property name="column">1</property>
          <property name="ipadx">5</property>
          <property name="ipady">0</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="btnRestart">
        <property name="command">restart</property>
        <property name="text" translatable="yes">Restart</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">2</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
